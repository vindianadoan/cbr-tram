# Transport Canberra GTFS-Realtime TripUpdates feed URL
# Example: https://example.gov.au/gtfs-rt/trip-updates
GTFS_RT_TRIP_UPDATES_URL=http://files.transport.act.gov.au/feeds/lightrail.pb

# If the feed requires auth, include the header value here
# e.g. "Bearer <token>" or an API key string as required by the endpoint
# This is sent as the Authorization header
GTFS_RT_API_KEY=

# Optional: HTTPS URL returning a JSON array of stops:
# [ { "id": "ALINGA", "name": "Alinga Street" }, ... ]
STOPS_URL=

# Fallback headway in seconds used when realtime is unavailable
FALLBACK_HEADWAY_SECONDS=360
